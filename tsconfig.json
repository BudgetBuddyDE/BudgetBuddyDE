{
  "compilerOptions": {
    "target": "es2017",
    "lib": ["es6"],
    "module": "node16",
    "resolveJsonModule": true,
    "allowJs": true,
    "outDir": "build",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "jsx": "react"
  },
  "include": ["./src", "./transactional"]
}
