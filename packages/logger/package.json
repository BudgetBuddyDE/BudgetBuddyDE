{
  "name": "@budgetbuddyde/logger",
  "version": "0.0.2",
  "description": "",
  "keywords": [
    "logger.js",
    "logging",
    "utility"
  ],
  "author": "Thorben Klein",
  "license": "MIT",
  "homepage": "https://github.com/budgetbuddyde/logger",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/budgetbuddyde/logger.git"
  },
  "bugs": {
    "url": "https://github.com/budgetbuddyde/logger/issues"
  },
  "scripts": {
    "build": "rimraf lib && tsc",
    "pretest": "npm run check",
    "test": "vitest run --silent",
    "test:watch": "vitest",
    "check": "biome check --error-on-warnings",
    "check:write": "npm run check -- --write --unsafe",
    "lint": "biome lint --write",
    "format": "biome format --write"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.0",
    "@types/node": "^24.0.14",
    "@vitest/ui": "^3.0.9",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.3",
    "vitest": "^3.0.9"
  },
  "peerDependencies": {
    "winston": "^3.19.0"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "dev-source": "./src/index.ts",
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./utils": {
      "dev-source": "./src/utils.ts",
      "types": "./lib/utils.d.ts",
      "default": "./lib/utils.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./lib/index.d.js"
      ],
      "utils": [
        "./lib/utils.d.js"
      ]
    }
  }
}
